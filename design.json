    "examples": {
      "buttonPrimary": "bg-primary text-primary-foreground hover:bg-primary/90",
      "cardLayout": "bg-card rounded-[var(--radius)] shadow-panel p-6",
      "textHierarchy": "text-foreground for primary, text-muted for secondary"
    }
  }
